{
  "name": "jk-node-website",
  "version": "1.0.0",
  "description": "A basic setup for my website",
  "author": "Jonathan Kolb <kolb0401@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run server",
    "start:dev": "npm run server:dev",
    "rimraf": "rimraf",
    "clean": "npm cache clean && npm run rimraf -- node_modules dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:common",
    "server:dev": "NODE_ENV=development npm run server:common",
    "server:prod": "NODE_ENV=production npm run server:common",
    "server:common": "node src/server/index.js"
  },
  "dependencies": {
    "babel-preset-es2015": "6.14.0",
    "core-js": "2.4.0",
    "express": "4.14.0",
    "http-proxy": "1.14.0",
    "react": "15.3.1",
    "react-dom": "15.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "es6-promise-loader": "^1.0.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.9.0",
    "parse5": "^1.5.1",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "svg-react-loader": "^0.3.7",
    "to-string-loader": "^1.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
